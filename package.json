{
    "name": "expressplate",
    "version": "1.1.0",
    "description": "Express.js REST API opinionated boilerplate",
    "author": "piotr@piech.dev",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Tenemo/expressplate.git"
    },
    "bugs": {
        "url": "https://github.com/Tenemo/expressplate/issues"
    },
    "homepage": "https://github.com/Tenemo/expressplate#readme",
    "engines": {
        "node": ">=16.14"
    },
    "type": "module",
    "scripts": {
        "start": "node dist/server.js",
        "start:prod": "cross-env NODE_ENV=production PORT=80 node dist/server.js",
        "prebuild": "npm run eslint:fix && npm run tsc && npm test && npm run cleanDist",
        "build": "cross-env NODE_ENV=production babel --extensions '.ts' src --out-dir dist",
        "build:skip": "npm run cleanDist && cross-env NODE_ENV=production babel --extensions '.ts' src --out-dir dist",
        "cleanDist": "rimraf ./dist && mkdir dist",
        "nodemon": "nodemon",
        "dev:ts": "tsx src/server.ts",
        "eslint": "eslint . --ext js,ts",
        "eslint:fix": "eslint . --ext js,ts --fix",
        "tsc": "tsc",
        "test": "cross-env NODE_ENV=test jest --passWithNoTests",
        "test:coverage": "npm run test -- --coverage",
        "test:watch": "npm run test -- --watch",
        "prepare": "husky"
    },
    "dependencies": {
        "@sinclair/typebox": "^0.34.30",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^16.4.7",
        "env-schema": "^6.0.1",
        "express": "^4.21.2",
        "express-winston": "^4.2.0",
        "http-errors": "^2.0.0",
        "joi": "^17.13.3",
        "morgan": "^1.10.0",
        "parse-database-url": "^0.3.0",
        "rimraf": "^6.0.1",
        "winston": "^3.17.0",
        "winston-daily-rotate-file": "^5.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.26.4",
        "@babel/core": "^7.26.10",
        "@babel/node": "^7.26.0",
        "@babel/preset-env": "^7.26.9",
        "@babel/preset-typescript": "^7.26.0",
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.23.0",
        "@types/cookie-parser": "^1.4.8",
        "@types/cors": "^2.8.17",
        "@types/eslint": "^9.6.1",
        "@types/eslint-plugin-prettier": "^3.1.3",
        "@types/express": "^5.0.1",
        "@types/http-errors": "^2.0.4",
        "@types/jest": "^29.5.14",
        "@types/morgan": "^1.9.9",
        "@types/node": "^22.13.11",
        "@types/npm": "^7.19.3",
        "@types/prettier": "^3.0.0",
        "@typescript-eslint/parser": "^8.27.0",
        "@vitest/eslint-plugin": "^1.1.38",
        "babel-jest": "^29.7.0",
        "babel-plugin-module-resolver": "^5.0.2",
        "core-js": "^3.41.0",
        "eslint": "^9.23.0",
        "eslint-config-prettier": "^10.1.1",
        "eslint-import-resolver-typescript": "^4.2.2",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-only-error": "^1.0.2",
        "eslint-plugin-prettier": "^5.2.3",
        "eslint-plugin-security": "^3.0.1",
        "eslint-plugin-unused-imports": "^4.1.4",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "jest-cli": "^29.7.0",
        "nodemon": "^3.1.9",
        "prettier": "^3.5.3",
        "prettier-eslint": "^16.3.0",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.19.3",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.27.0"
    }
}
